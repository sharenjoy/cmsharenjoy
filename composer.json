{
    "name": "sharenjoy/cmsharenjoy",
    "description": "This is cms",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BootstrapCMS/sentry"
        }
    ],
    "require": {
        "laravel/framework": "5.0.*",
        "illuminate/html": "~5.0",
        "sharenjoy/core": "dev-master",
        "guzzlehttp/guzzle": "~4.0",
        "pda/pheanstalk": "~3.0",
        "predis/predis": "~1.0",
        "intervention/image": "2.*",
        "cartalyst/sentry": "~3.0",
        "teepluss/theme": "dev-master",
        "laracasts/utilities": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "barryvdh/laravel-debugbar": "~2.0",
        "fzaninotto/faker": "1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php -r \"copy('Envoy.blade.php.example', 'Envoy.blade.php');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
